			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/scripts/schema/initialize.js"
		},
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "取得Schema",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/scripts/schema/get_schema.js"
		},
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "TableRename",
			"skipFiles": [
				"<node_internals>/**"
			],
